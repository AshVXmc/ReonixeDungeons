[display]

window/size/width=1080
window/size/height=720
window/size/always_on_top=true
window/size/test_width=1080
window/size/test_height=720

[editor_plugins]

enabled=PoolStringArray(  )

[gdnative]

singletons=[  ]

[global]

window=false

